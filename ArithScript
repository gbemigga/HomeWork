import math


x = int(input("enter a number:"))

square: int = int(x * x) #Square of X
print ("The square is = ", square)

squareroot  = (math.sqrt(x))
print ("The squareroot is = ", squareroot)

cuberoot  = (x ** (1. / 3))  #Cuberoot of X
print ("The cuberoot is = ", cuberoot)

xdiv = (x / x) # Variable divided by variable
print ("The variable divided by itself is:" ,xdiv)


y= int(input("number raised to power:"))
xraisedi = x ** y
print ( x, "raised to power", y, "is:" ,xraisedi)